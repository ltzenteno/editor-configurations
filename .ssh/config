# This goes inside ~/.ssh/config

# Default account
Host github.com
  Hostname github.com
  PreferredAuthentications publickey
  IdentityFile /Users/ltzenteno/.ssh/id_rsa
  User git

# Personal acount
Host personal.github.com
  Hostname github.com
  PreferredAuthentications publickey
  IdentityFile /Users/ltzenteno/.ssh/id_rsa_personal
  User git
